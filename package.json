{
  "name": "bounty-board",
  "version": "1.0.0",
  "description": "Galactic bounty board website for all your bountyhunting needs.",
  "author": "John Lynn <john@otter.tech>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -r dotenv/config server/index.js --watch server",
    "dev:remigrate": "node -r dotenv/config ./utils/remigrate.js",
    "dev:generate:migration": "node -r dotenv/config ./utils/generateMigration.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node -r dotenv/config server/index.js",
    "generate": "nuxt generate",
    "test": "jest",
    "test:w": "jest --watch",
    "migrate": "node -r dotenv/config -e 'require(`./server/services/migrate.js`).up()'"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0",
    "core": "./server/core",
    "cross-env": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "knex": "^0.20.8",
    "lodash": "^4.17.15",
    "lodash-inflection": "^1.5.0",
    "nuxt": "^2.0.0",
    "sequelize": "^5.21.3",
    "sqlite3": "^4.1.1",
    "umzug": "^2.2.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-jest": "^24.1.0",
    "enquirer": "^2.3.4",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "glob": "^7.1.6",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "vue-jest": "^4.0.0-0"
  }
}
